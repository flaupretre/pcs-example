  <dir baseinstalldir="/" name="/">
   <file name="config.m4" role="src" />
   <file name="config.w32" role="src" />
   <file name="Makefile.frag" role="src" />
   <file name="php_ex1.c" role="src" />
   <file name="php_ex1.h" role="src" />
   <file name="README.md" role="doc"/>
   <dir name="php">
    <dir name="phpc">
     <file name="code.phpc" role="src" />
    </dir>
    <dir name="src">
     <file name="Dummy1.php" role="src" />
     <file name="Dummy2.php" role="src" />
     <file name="Dummy3.php" role="src" />
     <file name="Dummy4.php" role="src" />
     <file name="Dummy5.php" role="src" />
    </dir>
   </dir>
   <dir name="tests">
    <file name="autoload_ext_class.phpt" role="test" />
    <file name="class_exists.phpt" role="test" />
    <file name="c_to_php_call.phpt" role="test" />
    <file name="php_to_c_call.phpt" role="test" />
    <file name="reflection_001.phpt" role="test" />
   </dir>
  </dir>
 </contents>
 <dependencies>
  <required>
   <php>
    <min>5.3.0</min>
   </php>
   <pearinstaller>
    <min>1.4.0b1</min>
   </pearinstaller>
  </required>
 </dependencies>
 <providesextension>ex1</providesextension>
